<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<!-- Sequences for expense prepaid -->
		<record id="seq_petty_cash_request" model="ir.sequence">
			<field name="name">Petty Cash</field>
			<field name="code">petty.cash.expense</field>
			<field name="prefix">Petty-</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for expense prepaid -->
		
		<record id="seq_expense_prepaaid" model="ir.sequence">
			<field name="name">Expense Prepaid</field>
			<field name="code">expense.prepaid</field>
			<field name="prefix">-</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for advance claim -->
		
		<record id="seq_advance_claim" model="ir.sequence">
			<field name="name">Advance Claim</field>
			<field name="code">advance.claim</field>
			<field name="prefix">-</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for expense -->
		
		<record id="seq_hr_expense" model="ir.sequence">
			<field name="name">Hr Expense</field>
			<field name="code">hr.expense</field>
			<field name="prefix">-</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for account transfer -->
		<record id="seq_account_transfer" model="ir.sequence">
			<field name="name">Account Transfer</field>
			<field name="code">account.transfer</field>
			<field name="prefix">TRANS-</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for account payment -->
		
		<record id="seq_account_payment" model="ir.sequence">
			<field name="name">Account Payment</field>
			<field name="code">account.payment</field>
			<field name="prefix">-%(year)s-</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for account move -->
		
		<record id="seq_account_move" model="ir.sequence">
			<field name="name">Account Move</field>
			<field name="code">account.move</field>
			<field name="prefix">/</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

	</data>
</odoo>
